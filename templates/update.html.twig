{% extends 'base.html.twig' %}

{% block body %}
    <h2>Modifier une photo</h2>
    <div class="form-div">
    {{ form_start(form) }}
         {{ form_row(form.title)}}
         {{ form_row(form.img)}}
         {{ form_row(form.text)}}
         {{ form_row(form.note)}}
         {{ form_row(form.author)}}
         <button type="submit">Modifier</button>
    {{ form_end(form) }}
    </div>
{% endblock %}